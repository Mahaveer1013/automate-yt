# YouTube API Configuration
YOUTUBE_CLIENT_ID=your_client_id_here
YOUTUBE_CLIENT_SECRET=your_client_secret_here
YOUTUBE_REFRESH_TOKEN=your_refresh_token_here
YOUTUBE_CHANNEL_ID=your_channel_id_here

# Application Configuration
NODE_ENV=production
# DATABASE_URL=postgresql://user:password@localhost:5432/youtube_automation
# OR for SQLite:
# DATABASE_URL=sqlite:./data/automation.db

# Rate Limiting
YOUTUBE_API_MAX_CALLS_PER_DAY=10000
GOOGLE_TRENDS_RATE_LIMIT_MS=1000

# Video Generation
DEFAULT_VIDEO_DURATION_MIN=5
DEFAULT_VIDEO_DURATION_MAX=15
BACKGROUND_MUSIC_VOLUME=0.3

# Thumbnail Settings
THUMBNAIL_WIDTH=1280
THUMBNAIL_HEIGHT=720

# Analytics
MINIMUM_CTR_TARGET=0.04
MINIMUM_RETENTION_TARGET=0.6
MINIMUM_RPM_TARGET=2.0

# AI / Content Generation
OPENAI_API_KEY=your_openai_api_key
ELEVENLABS_API_KEY=your_elevenlabs_api_key

# Paths
ASSETS_PATH=./src/assets
OUTPUT_PATH=./output

